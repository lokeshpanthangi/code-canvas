{
  "rewrites": [
    {
      "source": "/api/openai/:path*",
      "destination": "https://api.openai.com/:path*"
    },
    {
      "source": "/api/onecompiler",
      "destination": "https://onecompiler.com/api/v1/run"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
